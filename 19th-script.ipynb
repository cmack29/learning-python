    "\n",
    "print(name)\n",
    "\n",
    "first_name = name[0:5].upper()\n",
    "# if starting at 0, 0 index not needed\n",
    "last_name = name[6:].upper()\n",
    "last_character = name[-1]\n",
    "\n",
    "print(first_name)\n",
    "print(last_name)\n",
    "print(last_character)"
